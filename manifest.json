{
  "manifest_version": 3,
  "name": "G5 Bugfixes & Improvements",
  "version": "0.0.1",
  "description": "Makes some improvements to the G5 platform, while also fixing some minor bugs.",
  "author": "Lake Straly",
  "content_scripts": [
    {
      "matches": [
        "https://hub.g5marketingcloud.com/admin/clients/*"
      ],
      "js": [
        "js/hub/hub_client_page.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://hub.g5marketingcloud.com/admin/clients/*/locations/*"
      ],
      "js": [
        "js/hub/hub_location_page.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://cms.g5marketingcloud.com/clients/*/websites/*"
      ],
      "js": [
        "js/cms/cms_general.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "https://cms.g5marketingcloud.com/clients/*/websites/*/pages/*"
      ],
      "js": [
        "js/cms/cms_page.js"
      ]
    },
    {
      "matches": [
        "https://provisioner.g5marketingcloud.com/status/clients/*"
      ],
      "js": [
        "js/provisioner/provisioner_client_page.js"
      ]
    },
    {
      "matches": [
        "https://provisioner.g5marketingcloud.com/status/sites/*"
      ],
      "js": [
        "js/provisioner/provisioner_site_page.js"
      ]
    }
  ],
  "action": {
    "default_title": "G5 Bugfixes & Improvements"
  },
  "permissions": [
    "tabs"
  ]
}